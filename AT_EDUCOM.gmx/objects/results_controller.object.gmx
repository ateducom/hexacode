            <string>draw_set_alpha(alpha)
alpha -= 1/(room_speed*3)
draw_set_color(c_black)
draw_rectangle(0, 0, room_width, room_height, 0)
draw_set_alpha(1)

draw_set_font(font_medium)
draw_set_color(c_black)
draw_set_halign(fa_center)
draw_set_valign(fa_center)
draw_text(room_width/2, 400, global.mode)

if (level &gt;= 1)
{
draw_set_font(fnt0)
draw_set_halign(fa_left)
if global.mode = "Math Game" || global.mode = "Rhythm Game"
draw_text(room_width*0.1, room_height*0.7, "Correct Hits: " + string(global.corrects))
else if global.mode = "Clock Game"
draw_text(room_width*0.1, room_height*0.7, "Points Scored: ")
}
if (level &gt;= 2)
{
if global.mode = "Math Game"
draw_text(room_width*0.1, room_height*0.8, "Wrong Hits: " + string(global.wrongs))
else if global.mode = "Clock Game"
draw_text(room_width*0.1, room_height*0.8, string(global.points))
else if global.mode = "Rhythm Game"
draw_text(room_width*0.1, room_height*0.8, "Misses: " + string(global.wrongs))
}
if (level &gt;= 3)
{
if global.mode = "Math Game"
    if global.corrects &gt; 4
    {
        victory = 1
        global.math = 1
    }
    else if global.corrects &gt; 0
    {
        victory = 2
        global.math = 1
    }
    else victory = 0
else if global.mode = "Clock Game"
    if global.points &gt; 800
    {
        victory = 1
        global.patience = 1
        global.focus = 1
    }
    else if global.points &gt; 300
    {
        victory = 2
        global.patience = 1
    }
    else victory = 0
else if global.mode = "Rhythm Game"
    if global.corrects &gt; 20
    {
        victory = 1
        global.fast = 1
    }
    else if global.corrects &gt; 10
    {
        victory = 2
        global.fast = 1
    }
    else victory = 0
    
    if victory = 1
    draw_sprite(spr_results, 1, room_width/2, 0)
    else if victory = 2
    draw_sprite(spr_results, 0, room_width/2, 0)
    else if victory = 0
    draw_sprite(spr_results, 2, room_width/2, 0)
}
if (level &gt;= 4)
{
background_visible[1] = 1
background_hspeed[1] = 3
background_blend[1] = c_black
}
</string>
